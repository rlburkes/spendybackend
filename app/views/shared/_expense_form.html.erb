<%= form_for @expense, :builder => StandardFormBuilder do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.label :amount %>
    <%= f.text_field :amount, :type => 'number'%>
    <%= f.label :tags %>
    <%=f.text_field_with_list :tag_list, :label => "Tag List", :datalist => @tags %>
    <ul>
  </div>
  <%= f.submit "Post", class: "btn btn-large btn-primary" %>
<% end %>